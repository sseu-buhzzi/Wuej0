<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
>
	<com.buhzzi.wuej_0.wuej_maps_view
		android:id="@+id/maps_view"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
	/>
	<GridLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_margin="64px"
		android:layout_alignParentBottom="true"
		android:rowCount="3"
		android:columnCount="3"
	>
		<ImageView
			android:id="@+id/cntl_cc_ImageView"
			android:layout_row="1"
			android:layout_column="1"
		/>

		<ImageView
			android:id="@+id/cntl_arrow_right_ImageView"
			android:layout_row="1"
			android:layout_column="2"
		/>
		<ImageView
			android:id="@+id/cntl_arrow_up_ImageView"
			android:layout_row="0"
			android:layout_column="1"
			android:rotation="270"
		/>
		<ImageView
			android:id="@+id/cntl_arrow_left_ImageView"
			android:layout_row="1"
			android:layout_column="0"
			android:rotation="180"
		/>
		<ImageView
			android:id="@+id/cntl_arrow_down_ImageView"
			android:layout_row="2"
			android:layout_column="1"
			android:rotation="90"
		/>

		<ImageView
			android:id="@+id/cntl_rota_rcw_ImageView"
			android:layout_row="2"
			android:layout_column="0"
		/>
		<ImageView
			android:id="@+id/cntl_rota_cw_ImageView"
			android:layout_row="0"
			android:layout_column="0"
			android:scaleY="-1"
		/>

		<ImageView
			android:id="@+id/cntl_zoom_in_ImageView"
			android:layout_row="0"
			android:layout_column="2"
		/>
		<ImageView
			android:id="@+id/cntl_zoom_out_ImageView"
			android:layout_row="2"
			android:layout_column="2"
		/>
	</GridLayout>
	<ImageView
		android:id="@+id/central_cursor_ImageView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerInParent="true"
	/>
</RelativeLayout>
